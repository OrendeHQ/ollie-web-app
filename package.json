{
  "name": "ollie",
  "version": "1.0.0",
  "description": "Ollie Web App",
  "main": "server/index.js",
  "repository": "git@github.com:OrendeHQ/ollie-web-app.git",
  "author": "Stanley Nguyen <hung.ngn.the@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run db:migrate && npm run build && node server",
    "build": "rimraf dist && webpack --config webpack.config.prod.js --progress --profile --colors",
    "dev": "rimraf dist && cross-env NODE_ENV=dev nodemon server --ignore client",
    "lint": "eslint .",
    "db:migrate": "knex migrate:latest --knexfile server/db/knexfile.js",
    "db:make": "knex migrate:make --knexfile server/db/knexfile.js",
    "db:rollback": "knex migrate:rollback --knexfile server/db/knexfile.js"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "bookshelf": "^0.10.3",
    "bookshelf-cascade-delete": "^2.0.1",
    "cross-env": "^5.0.0",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.28.0",
    "imgur": "^0.2.1",
    "jsonwebtoken": "^7.4.0",
    "knex": "^0.13.0",
    "multer": "^1.3.0",
    "pg": "^6.2.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "react-transition-group": "^1.1.3",
    "reactstrap": "^4.6.2",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1",
    "styled-components": "^1.4.6",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^7.0.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "next",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
